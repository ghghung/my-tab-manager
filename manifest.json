{
  "manifest_version": 3,
  "name": "My Tab Manager",
  "version": "2.3.5",
  "description": "Save and manage tabs efficiently. By anhtalaghung",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>",
    "*://suggestqueries.google.com/*",
    "*://s2.googleusercontent.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "My Tab Manager"
  },
  "chrome_url_overrides": {
    "newtab": "manager.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["home-button.css"],
      "js": ["home-button.js"]
    },
    {
      "matches": ["<all_urls>"],
      "css": ["spotlight.css"],
      "js": ["spotlight.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/icon48.png"],
      "matches": ["<all_urls>"]
    }
  ]  
}